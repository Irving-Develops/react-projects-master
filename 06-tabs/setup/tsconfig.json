{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["es6", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "baseUrl": "src",
    "noImplicitAny": false,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true
  },
  "include": ["src"]
}